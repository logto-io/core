{
  "name": "@logto/ui",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "precommit": "lint-staged",
    "start": "parcel src/index.html",
    "dev": "PORT=5001 parcel src/index.html --no-cache --hmr-port 6001",
    "check": "tsc --noEmit",
    "build": "pnpm check && rm -rf dist && parcel build src/index.html --no-autoinstall --no-cache",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:report": "pnpm lint -- --format json --output-file report.json",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "test": "jest"
  },
  "dependencies": {
    "@logto/phrases": "^0.1.0",
    "@logto/schemas": "^0.1.0",
    "@silverhand/essentials": "1.1.7",
    "classnames": "2.3.1",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.4",
    "js-base64": "3.7.2",
    "ky": "0.29.0",
    "libphonenumber-js": "1.9.51",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.16.7",
    "react-modal": "3.14.4",
    "react-phone-number-input": "3.1.50",
    "react-router-dom": "6.3.0",
    "react-string-replace": "1.0.0",
    "react-timer-hook": "3.0.5"
  },
  "devDependencies": {
    "@jest/types": "27.5.1",
    "@parcel/core": "2.5.0",
    "@parcel/transformer-sass": "2.5.0",
    "@peculiar/webcrypto": "1.3.3",
    "@silverhand/eslint-config": "0.10.2",
    "@silverhand/eslint-config-react": "0.10.3",
    "@silverhand/ts-config": "0.10.2",
    "@silverhand/ts-config-react": "0.10.3",
    "@testing-library/react": "12.1.5",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/react-modal": "3.13.1",
    "@types/react-router-dom": "5.3.3",
    "eslint": "8.13.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "11.2.6",
    "parcel": "2.5.0",
    "postcss": "8.4.12",
    "postcss-modules": "4.3.1",
    "prettier": "2.6.2",
    "stylelint": "13.13.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "eslintConfig": {
    "extends": "@silverhand/react"
  },
  "stylelint": {
    "extends": "@silverhand/eslint-config-react/.stylelintrc"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc"
}
