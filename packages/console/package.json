{
  "name": "@logto/console",
  "version": "0.1.0",
  "description": "> TODO: description",
  "author": "Gao Sun <gao@silverhand.io>",
  "homepage": "https://github.com/logto-io/logto#readme",
  "license": "MPL-2.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "precommit": "lint-staged",
    "copyfiles": "copyfiles -u 1 public/**/*.* dist",
    "start": "pnpm copyfiles && parcel src/index.html",
    "dev": "pnpm copyfiles && PORT=5002 parcel src/index.html --public-url /console --no-cache --hmr-port 6002",
    "check": "tsc --noEmit",
    "build": "pnpm check && rm -rf dist && pnpm copyfiles && parcel build src/index.html --no-autoinstall --no-cache --public-url /console",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:report": "pnpm lint -- --format json --output-file report.json",
    "stylelint": "stylelint \"src/**/*.scss\""
  },
  "dependencies": {
    "@logto/phrases": "^0.1.0",
    "@logto/react": "0.1.4",
    "@logto/schemas": "^0.1.0",
    "@monaco-editor/react": "4.4.2",
    "@silverhand/essentials": "1.1.7",
    "classnames": "2.3.1",
    "csstype": "3.0.11",
    "dnd-core": "16.0.1",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.4",
    "ky": "0.30.0",
    "lodash.kebabcase": "4.1.1",
    "monaco-editor": "0.32.1",
    "nanoid": "3.3.3",
    "react": "17.0.2",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "17.0.2",
    "react-hook-form": "7.30.0",
    "react-hot-toast": "2.2.0",
    "react-i18next": "11.16.7",
    "react-markdown": "8.0.3",
    "react-modal": "3.14.4",
    "react-paginate": "8.1.3",
    "react-router-dom": "6.3.0",
    "remark-gfm": "3.0.1",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@parcel/core": "2.5.0",
    "@parcel/transformer-sass": "2.5.0",
    "@silverhand/eslint-config": "0.10.2",
    "@silverhand/eslint-config-react": "0.10.3",
    "@silverhand/ts-config": "0.10.2",
    "@silverhand/ts-config-react": "0.10.3",
    "@types/lodash.kebabcase": "4.1.7",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/react-modal": "3.13.1",
    "copyfiles": "2.4.1",
    "eslint": "8.13.0",
    "lint-staged": "11.2.6",
    "parcel": "2.5.0",
    "postcss": "8.4.12",
    "postcss-modules": "4.3.1",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "stylelint": "13.13.1",
    "typescript": "4.6.3"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "eslintConfig": {
    "extends": "@silverhand/react"
  },
  "stylelint": {
    "extends": "@silverhand/eslint-config-react/.stylelintrc"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc"
}
